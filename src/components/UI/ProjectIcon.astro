---
import IconProjectJavascript from "@assets/icons/icon-project-javascript.astro";
import IconProjectReact from "@assets/icons/icon-project-react.astro";
import IconProjectAstro from "@assets/icons/icon-project-astro.astro";

const { icon } = Astro.props;
const React = "React";
---

{icon === "javascript" && <IconProjectJavascript />}
{icon === "react" && <IconProjectReact />}
{icon === "astro" && <IconProjectAstro />}